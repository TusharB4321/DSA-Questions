public class MinimumItem {
    public static void main(String[] args) {

        int[]arr={50,100,150,200,5,-30,3,2};
        System.out.println(SearchMaximum(arr));

    }

    static int SearchMaximum(int[]arr){

        if (arr.length==0){
            return -1;
        }
        int max=arr[0];
        for (int i=1;i<arr.length;i++){
            if (max<arr[i]){
                max=arr[i];
            }

        }
        return max;
    }
}
