public class MinimumItem {
    public static void main(String[] args) {

        int[]arr={50,-100,150,200,5,-30,3,2};
        System.out.println(SearchMinimum(arr));

    }

    static int SearchMinimum(int[]arr){

        if (arr.length==0){
            return -1;
        }
        int min=arr[0];
        for (int i=1;i<arr.length;i++){
            if (min>arr[i]){
                min=arr[i];
            }

        }
        return min;
    }
}
